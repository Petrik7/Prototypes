<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    @model ConfigForm.Models.ConfigData
    @*@model IEnumerable<ConfigForm.Models.ConfigItem>*@

    <script type="text/javascript">
        someFunctionLay()
        {
            alert('someFunction lay is called.');
        }
        
        </script>

</head>
<body>

<div class="page">
    <header class="common_header">
        <div id="title">
            <h2>Configuration Form</h2>
        </div>
        <div id="logindisplay">
            @Html.Partial("_LogOnPartial")
        </div>
    </header>
          
    <div class="main_wrapper">

        <div class="left_l1">
            @{Html.RenderPartial("~/Views/Shared/ConfigItems.cshtml", Model.ConfigItems);}
        </div>

        <div class="right_l1">
            <div>
               @RenderBody()
            </div>
        </div>
    </div>
    <footer>
        footer
    </footer>        
</div>
</body>
</html>

